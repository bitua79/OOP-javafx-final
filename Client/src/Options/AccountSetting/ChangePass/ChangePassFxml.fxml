            <Font size="10.0" />
         </font>
      </TextField>
      <TextField id="Wrong-text-field" fx:id="conNwPassField" layoutX="350.0" layoutY="173.0" prefHeight="27.0" prefWidth="147.0" promptText="confirm your new password" stylesheets="@../../Main/OptionsStyle.css">
