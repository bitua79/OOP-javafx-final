<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchorPane" styleClass="AnchorPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogIn.RetrievalPass.RetrievalPassCon">
    <children>
        <ImageView fx:id="imageView" fitHeight="809.0" fitWidth="654.0" layoutX="624.0" layoutY="104.0" opacity="0.85" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../Resources/Photos/RetrievalPass.png" />
            </image>
        </ImageView>
        <TextField fx:id="nameField" layoutX="760.0" layoutY="397.0" onKeyPressed="#handle" onKeyReleased="#handle" prefHeight="61.0" prefWidth="250.0" stylesheets="@RetrievalPassStyle.css" />
        <TextField fx:id="answerField" layoutX="763.0" layoutY="568.0" onKeyPressed="#handle" onKeyReleased="#handle" prefHeight="61.0" prefWidth="250.0" stylesheets="@RetrievalPassStyle.css" />
        <Label fx:id="nameLbl" layoutX="680.0" layoutY="388.0" stylesheets="@RetrievalPassStyle.css" text="name:">
            <font>
                <Font size="34.0" />
            </font>
        </Label>
        <Label fx:id="answerLbl" layoutX="669.0" layoutY="563.0" stylesheets="@RetrievalPassStyle.css" text="answer:">
            <font>
                <Font size="34.0" />
            </font>
        </Label>
        <Label fx:id="questionLbl" layoutX="656.0" layoutY="483.0" stylesheets="@RetrievalPassStyle.css" text="question:">
            <font>
                <Font size="34.0" />
            </font>
        </Label>
        <ChoiceBox id="choice" fx:id="questionBox" layoutX="759.0" layoutY="473.0" prefHeight="69.0" prefWidth="250.0" stylesheets="@../ChoiceStyle.css" value="Favorite color">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Favorite color" />
                    <String fx:value="Favorite pet" />
                    <String fx:value="Favorite food" />
                </FXCollections>
            </items>
        </ChoiceBox>
        <Button fx:id="enter" disable="true" layoutX="789.0" layoutY="794.0" mnemonicParsing="false" onAction="#FindPass" stylesheets="@RetrievalPassStyle.css" text="Find password" />
        <Label id="reportRecover" fx:id="report" ellipsisString="" layoutX="651.0" layoutY="672.0" prefHeight="50.0" prefWidth="334.0" stylesheets="@RetrievalPassStyle.css" textAlignment="CENTER" textOverrun="CLIP" wrapText="true">
            <font>
                <Font size="48.0" />
            </font>
        </Label>
        <Button fx:id="back" layoutX="651.0" layoutY="141.0" mnemonicParsing="false" onAction="#backToSignIn" stylesheets="@RetrievalPassStyle.css" text="Back" />
    </children>
</AnchorPane>
